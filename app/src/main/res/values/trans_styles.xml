<resources>
    <style name="layout_trans_select">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">52dp</item>
    </style>
    <style name="tv_trans_select_title">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/text_size_14</item>
        <item name="android:textColor">@color/text_color_white</item>
    </style>
    <style name="trans_view_line">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">3dp</item>
        <item name="android:background">@color/tab_select_white</item>
    </style>
    <style name="home_title_select">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">3dp</item>
        <item name="android:background">@color/blue_color</item>
    </style>
    <style name="trans_check_box" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_12</item>
        <item name="android:textColor">@color/text_color_black</item>
        <item name="android:layout_marginLeft">@dimen/margin_padding_15</item>
        <item name="android:paddingLeft">@dimen/margin_padding_5</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:button">@drawable/trans_check_box_style</item>
    </style>
    <style name="trans_rl" parent="match_wrap">
        <item name="android:padding">@dimen/margin_padding_8</item>
        <item name="android:background">@color/background_color_white</item>
    </style>
    <style name="trans_listView" parent="match_wrap">
        <item name="android:dividerHeight">0dp</item>
        <item name="android:divider">@color/background_color</item>
        <item name="android:scrollbars">none</item>
    </style>
    <style name="trans_ed">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:background">@drawable/edit_select</item>
        <item name="android:textSize">@dimen/text_size_14</item>
        <item name="android:textColor">@color/text_color_black</item>
        <item name="android:textColorHint">@color/text_color_gray</item>
    </style>
    <style name="trans_asset_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/w_0.1</item>
        <item name="android:background">@color/vertical_line_color</item>
    </style>
    <style name="btn_trans_buysell" >
        <item name="android:padding">@dimen/margin_padding_5</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/margin_padding_35</item>
        <item name="android:textSize">@dimen/text_size_14</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>
    <style name="btn_trans_buy_cpb" parent="btn_trans_buysell">

    </style>
    <style name="btn_trans_buy" parent="btn_trans_buysell">
        <item name="android:textColor">@color/tv_click_trans_buy</item>
        <item name="android:background">@drawable/bnt_trans_sell</item>
    </style>
    <style name="btn_trans_sell_cpb" parent="btn_trans_buysell">

    </style>
    <style name="btn_trans_sell" parent="btn_trans_buysell">
        <item name="android:textColor">@color/tv_click_trans_sell</item>
        <item name="android:background">@drawable/bnt_trans_buy</item>
    </style>

    <style name="tv_trans_rde_l" parent="tv_user_red_14">
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="tv_trans_gray_l" parent="tv_user_gray_14">
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="tv_trans_rde_r" parent="tv_user_red_14">
        <item name="android:layout_marginLeft">@dimen/margin_padding_5</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="tv_trans_black_r" parent="tv_user_black_14">
        <item name="android:layout_marginLeft">@dimen/margin_padding_5</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="tv_market_b" parent="wrap_wrap">
        <item name="android:textColor">@color/text_color_green</item>
        <item name="android:textSize">@dimen/text_size_14</item>
    </style>
    <style name="tv_market_r" parent="wrap_wrap">
        <item name="android:textColor">@color/text_color_red</item>
        <item name="android:textSize">@dimen/text_size_14</item>
    </style>
    <style name="StyleProgressBarMini" parent="@android:style/Widget.ProgressBar.Horizontal">

        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@android:drawable/progress_indeterminate_horizontal</item>
        <item name="android:progressDrawable">@drawable/progress_horizontal</item>
    </style>

    <style name="spinner_safety_black" parent="weight_match">
        <item name="android:layout_weight">2</item>
        <item name="android:background">@null</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_height">@dimen/margin_padding_30</item>
    </style>
    <style name="spinner_safety" parent="wrap_wrap">
        <item name="android:background">@null</item>
    </style>
    <style name="but_trans_qx" parent="wrap_wrap">
        <item name="android:layout_margin">@dimen/margin_padding_5</item>
        <item name="android:layout_height">@dimen/margin_padding_25</item>
        <item name="android:background">@drawable/bnt_login</item>
        <item name="android:textSize">@dimen/text_size_12</item>
        <item name="android:textColor">@color/tv_click_textcolor_w_to_h</item>
    </style>
</resources>
