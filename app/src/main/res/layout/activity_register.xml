    <LinearLayout
        style="@style/login_sign_up_layout">

        <RelativeLayout
            android:id="@+id/rl_xzqh"
            style="@style/match_wrap">

            <TextView
                android:id="@+id/ed_qh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:text="@string/safety_zgdl"
                android:drawableLeft="@mipmap/ico_earth"
                style="@style/user_edit_login" />

            <ImageView
                style="@style/img_user_login_r"
                android:src="@mipmap/ico_arrowright" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/line_market_gray"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/ed_name"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:maxLength="11"
                android:typeface="monospace"
                android:inputType="number"
                android:hint="@string/user_sjhm_hint"
                android:drawableLeft="@mipmap/ico_mobile"
                style="@style/user_edit_login" />

            <ImageView
                android:id="@+id/clearEdNameImage"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:visibility="gone"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:background="@mipmap/user_clear"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/line_market_gray"/>

        <LinearLayout
            android:id="@+id/linearLayout_pwd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <EditText
                android:id="@+id/ed_pwd"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:typeface="monospace"
                android:inputType="textPassword"
                android:hint="@string/user_szdlmm"
                android:layout_toLeftOf="@+id/img_eyes_close"
                android:drawableLeft="@mipmap/ico_lock"
                style="@style/user_edit_login"  />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/line_market_gray"/>

        <EditText
            android:id="@+id/ed_pwd2"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:inputType="textPassword"
            android:typeface="monospace"
            android:hint="@string/user_qrdlmm"
            android:layout_toLeftOf="@+id/img_eyes_close2"
            android:drawableLeft="@mipmap/ico_lock"
            style="@style/user_edit_login"  />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/line_market_gray"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp">

            <EditText
                android:id="@+id/ed_code"
                android:layout_width="220dp"
                android:layout_height="wrap_content"
                android:maxLength="6"
                android:typeface="monospace"
                android:inputType="number"
                android:hint="@string/user_dxyzm"
                android:layout_toLeftOf="@+id/bnt_code"
                android:drawableLeft="@mipmap/ico_lock"
                style="@style/user_edit_login" />

            <View
                android:layout_width="0.5dp"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@color/line_market_gray"/>

            <Button
                android:id="@+id/bnt_code"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#00000000"
                android:gravity="center"
                android:text="@string/user_hqyzm"
                android:textSize="14sp"
                android:textColor="@color/text_color_blue"/>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/line_market_gray"/>


        <Button
            android:id="@+id/btn_register"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginTop="18dp"
            android:background="@drawable/btn_sign_up_bg"
            android:text="@string/user_next"
            android:textSize="16sp"
            android:textColor="@color/white" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center|bottom"
            android:paddingBottom="38dp">

            <TextView
                android:id="@+id/use_email_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="bottom|center_horizontal"
                android:text="@string/user_register_by_email"
                android:textColor="@color/bottom_blue"
                android:textSize="14sp"
                android:textStyle="bold"/>

        </LinearLayout>

