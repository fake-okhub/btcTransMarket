<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/trans_exchange_buy_colorAccent">
    <item
        android:id="@android:id/mask" >
        <shape android:shape="rectangle">
            <solid android:color="@color/trans_exchange_buy_ripple" />
            <corners android:radius="@dimen/radius_1" />
            <stroke
                android:color="@color/trans_exchange_buy_ripple"
                android:width="1px" />
        </shape>
    </item>
    <item
        android:id="@android:id/background" >
        <shape android:shape="rectangle">
            <solid android:color="@color/trans_exchange_buy" />
            <corners android:radius="@dimen/radius_1" />
            <stroke
                android:color="@color/trans_exchange_buy"
                android:width="1px" />
        </shape>
    </item>
</ripple>