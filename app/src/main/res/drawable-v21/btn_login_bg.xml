<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/btn_login_bg_colorAccent">
    <item
        android:id="@android:id/mask" >
        <shape android:shape="rectangle">
            <solid android:color="@color/btn_login_bg_ripple" />
            <corners android:radius="@dimen/radius_1" />
            <stroke
                android:color="@color/btn_login_bg_ripple"
                android:width="1px" />
        </shape>
    </item>
    <item
        android:id="@android:id/background" >
        <shape android:shape="rectangle">
            <solid android:color="@color/btn_login_bg" />
            <corners android:radius="@dimen/radius_1" />
            <stroke
                android:color="@color/btn_login_bg"
                android:width="1px" />
        </shape>
    </item>
</ripple>